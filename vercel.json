{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/markdown",
      "destination": "/api/markdown"
    },
    {
      "source": "/api/notebooks",
      "destination": "/api/notebooks"
    },
    {
      "source": "/api/notebook-content",
      "destination": "/api/notebook-content"
    },
    {
      "source": "/(.*)",
      "destination": "/api/check-curl?path=$1"
    }
  ]
}
