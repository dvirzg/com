{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/markdown",
      "destination": "/api/markdown"
    },
    {
      "source": "/api/send-tweet-reply",
      "destination": "/api/send-tweet-reply"
    },
    {
      "source": "/(.*)",
      "destination": "/api/check-curl?path=$1"
    }
  ]
}
